{
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node $CLAUDE_PLUGIN_ROOT/dist/hooks/session-start.js",
            "timeout": 10
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node $CLAUDE_PLUGIN_ROOT/dist/hooks/session-end.js",
            "timeout": 30
          }
        ]
      }
    ],
    "Periodic": [
      {
        "intervalMinutes": 5,
        "hooks": [
          {
            "type": "command",
            "command": "node $CLAUDE_PLUGIN_ROOT/dist/hooks/periodic-save.js",
            "timeout": 15
          }
        ]
      }
    ]
  }
}
